template<class P>
inline IppStatus ippiMirror_C1R(const P* pSrc, int srcStep, P* pDst, int dstStep,
                                      IppiSize roiSize, IppiAxis flip) {
 if(ippAxsHorizontal) {
    for(int j=0;j<roiSize.height;j++) {
        const P* src=pSrc+srcStep*j;
        P* dst=pDst+dstStep*j+roiSize.width-1;
        for(int i=0;i<roiSize.width;i++,src++,dst--) {
            *dst=*src;
        }
    }
 } else if(ippAxsVertical) {
    for(int j=0;j<roiSize.height;j++) {
        const P* src=pSrc+srcStep*j;
        P* dst=pDst+dstStep*(roiSize.height-j-1);
        for(int i=0;i<roiSize.width;i++,src++,dst++) {
            *dst=*src;
        }
    }
 } else {
    for(int j=0;j<roiSize.height;j++) {
        const P* src=pSrc+srcStep*j;
        P* dst=pDst+dstStep*(roiSize.height-j-1)+roiSize.width-1;
        for(int i=0;i<roiSize.width;i++,src++,dst--) {
            *dst=*src;
        }
    }
 }
 return ippStsNoErr;
}
