template<class P>
inline IppStatus ippiMirror_C1IR(P* pSrcDst, int srcDstStep, IppiSize roiSize, IppiAxis flip) {
 if(ippAxsHorizontal) {
    for(int j=0;j<roiSize.height;j++) {
        P* src=pSrcDst+srcDstStep*j;
        P* dst=pSrcDst+srcDstStep*j+roiSize.width-1;
        for(int i=0;i<roiSize.width/2;i++,src++,dst--) {
            std::swap(dst,src);
        }
    }
 } else if(ippAxsVertical) {
    for(int j=0;j<roiSize.height/2;j++) {
        P* src=pSrcDst+srcDstStep*j;
        P* dst=pSrcDst+srcDstStep*(roiSize.height-j-1);
        for(int i=0;i<roiSize.width;i++,src++,dst++) {
            std::swap(dst,src);
        }
    }
 } else {
    for(int j=0;j<roiSize.height/2;j++) {
        P* src=pSrcDst+srcDstStep*j;
        P* dst=pSrcDst+srcDstStep*(roiSize.height-j-1)+roiSize.width-1;
        for(int i=0;i<roiSize.width;i++,src++,dst--) {
            std::swap(dst,src);
        }
    }
    if(roiSize.height%2==1) {
        int j=roiSize.height/2+1;
        P* src=pSrcDst+srcDstStep*j;
        P* dst=pSrcDst+srcDstStep*(roiSize.height-j-1)+roiSize.width-1;
        for(int i=0;i<roiSize.width/2;i++,src++,dst--) {
            std::swap(dst,src);
        }
    }
 }
 return ippStsNoErr;
}
