inline IppStatus ippiMulC_C1RSfs(const Ipp16s* pSrc, int srcStep, Ipp16s value,
                                 Ipp16s* pDst, int dstStep, IppiSize roiSize, int scaleFactor) 
{
    #ifdef NICE_USELIB_IPP
        return ippiMulC_16s_C1RSfs(pSrc,srcStep,value,pDst,dstStep,roiSize,scaleFactor);
    #else
        // ipp needs scaleFactor==0 if no scaling is wanted, so we have to work around here
        int tscaleFactor = (scaleFactor==0)?1:scaleFactor;

        const Ipp16s* pS;
              Ipp16s* pD;
        int sStep = srcStep/sizeof(Ipp16s);
        int dStep = dstStep/sizeof(Ipp16s);
        for(int y=0; y<roiSize.height; ++y)
        {
            pS = pSrc + y*sStep;
            pD = pDst + y*dStep;
            for(int x=0; x<roiSize.width; ++x,++pS,++pD)
                *pD = (*pS*value)/tscaleFactor;
        }
        return ippStsNoErr;
    #endif
}

inline IppStatus ippiSub_C1IR(const Ipp32f* pSrc, int srcStep,
                              Ipp32f* pDst, int dstStep, IppiSize roiSize)
{
    #ifdef NICE_USELIB_IPP
        return ippiSub_32f_C1IR(pSrc,srcStep,pDst,dstStep,roiSize);
    #else
        const Ipp32f* pS;
              Ipp32f* pD;
        int sStep = srcStep/sizeof(Ipp32f);
        int dStep = dstStep/sizeof(Ipp32f);
        for(int y=0; y<roiSize.height; ++y)
